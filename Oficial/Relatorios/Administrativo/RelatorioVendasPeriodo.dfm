  object SQLVendasConsig: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select'
      '  CUPOM.CLIEA13ID,'
      '  CUPOM.CUPOA13ID,'
      '  CUPOM.CUPOCSTATUS,'
      '  CUPOM.CUPOCTIPOPADRAO,'
      '  CUPOM.CUPODCANC,'
      '  CUPOM.CUPODEMIS,'
      '  CUPOM.CUPON2TOTITENS,'
      '  CUPOM.CUPON2ACRESC,'
      '  CUPOM.CUPON2DESC,'
      
        '  CUPOM.CUPON2TOTITENS + CUPON3CREDTAXA + CUPOM.CUPON2ACRESC-CUP' +
        'OM.CUPON2DESC-CUPOM.CUPON3BONUSTROCA-CUPOM.CUPON2TOTITENSRET as ' +
        'TOTAL,'
      '  CUPOM.EMPRICOD,'
      '  CUPOM.PLRCICOD,'
      '  CUPOM.VENDICOD,'
      '  CUPOM.TERMICOD,'
      '  CLIENTE.CLIEA60RAZAOSOC,'
      '  VENDEDOR.VENDA60NOME'
      'from'
      
        '  (CUPOM CUPOM inner join CLIENTE CLIENTE on CUPOM.CLIEA13ID = C' +
        'LIENTE.CLIEA13ID)'
      
        '  inner join VENDEDOR VENDEDOR on CUPOM.VENDICOD = VENDEDOR.VEND' +
        'ICOD'
      'where'
      '  CUPOM.CUPOCSTATUS = '#39'A'#39' and'
      '  CUPOM.CUPOCCONSIG = '#39'S'#39' and'
      '  %MEmpresa and'
      '  %MData and'
      '  %MVendedor'
      'order by'
      '  CUPOM.CUPODEMIS asc,'
      '  CUPOM.CUPOA13ID asc')
