  object SQLVendas: TRxQuery
    DatabaseName = 'DB'
    SQL.Strings = (
      'select'
      '  CUPOM.CLIEA13ID,'
      '  CUPOM.CUPOA13ID,'
      '  CUPOM.CUPOCSTATUS,'
      '  CUPOM.CUPOCTIPOPADRAO,'
      '  CUPOM.CUPODCANC,'
      '  CUPOM.CUPODEMIS,'
      '  (select sum(CUPOMITEM.TOTAL_ITEM)'
      '        from CUPOMITEM'
      '        where CUPOMITEM.CUPOA13ID = CUPOM.CUPOA13ID and'
      '              (%MStatus) and'
      '              CUPOMITEM.CPITN3QTD > 0'
      '        group by CUPOMITEM.CUPOA13ID)  as CUPON2TOTITENS,'
      '  CUPOM.CUPON2ACRESC,'
      '  CUPOM.CUPON2DESC + CUPOM.CUPON2DESCITENS as CUPON2DESC,'
      '  CUPOM.CUPOA8PLACAVEIC,'
      '  (select sum(CUPOMITEM.TOTAL_ITEM)'
      '        from CUPOMITEM'
      '        where CUPOMITEM.CUPOA13ID = CUPOM.CUPOA13ID and'
      '              (%MStatus) and'
      '              CUPOMITEM.CPITN3QTD > 0'
      '        group by CUPOMITEM.CUPOA13ID) +'
      '        CUPOM.CUPON2ACRESC'
      '        %MtaxaCred'
      '        CUPOM.CUPON2DESCITENS-'
      '        CUPOM.CUPON3BONUSTROCA-'
      '        CUPOM.CUPON2TOTITENSRET as TOTAL,'
      '  CUPOM.CUPON3BONUSTROCA,'
      '  (select'
      '     sum(CUPOMITEM.CPITN3QTD)'
      '   from'
      '     CUPOMITEM'
      '   where'
      '     CUPOMITEM.CUPOA13ID = CUPOM.CUPOA13ID and'
      '     (%MStatus) and'
      '     CUPOMITEM.CPITN3QTD > 0'
      '   group by'
      '     CUPOMITEM.CUPOA13ID) as NROITENS,'
      '     CUPOM.EMPRICOD,'
      '     CUPOM.PLRCICOD,'
      '     CUPOM.VENDICOD,'
      '     CUPOM.TERMICOD,'
      '     CLIENTE.CLIEA60RAZAOSOC,'
      '     PLANORECEBIMENTO.PLRCA60DESCR,'
      '     VENDEDOR.VENDA60NOME,'
      '     CUPOM.CUPOINRO as DOC'
      'from'
      '  CUPOM'
      
        '  left outer join CLIENTE CLIENTE on CUPOM.CLIEA13ID = CLIENTE.C' +
        'LIEA13ID'
      
        '  left outer join PLANORECEBIMENTO PLANORECEBIMENTO on CUPOM.PLR' +
        'CICOD = PLANORECEBIMENTO.PLRCICOD'
      
        '  left outer join VENDEDOR VENDEDOR on CUPOM.VENDICOD = VENDEDOR' +
        '.VENDICOD'
      'where'
      '  (%MStatus) and'
      '  CUPOM.CUPOCCONSIG <> '#39'S'#39' and'
      '  (%MEmpresa) and'
      '  (%MTerminal) and'
      '  (%MData) and'
      '  (%MVendedor) and'
      '  (%MPlaca) and'
      '  (%MTipoPadrao) and'
      '  (%MCliente) and'
      '  (%MFaixaValor)'
      'order by'
      '  CUPOM.CUPODEMIS asc,'
      '  CUPOM.CUPOA13ID asc')
    Macros = <
      item
        DataType = ftString
        Name = 'MStatus'
        ParamType = ptInput
        Value = '0=0'
      end
      item
        DataType = ftString
        Name = 'MtaxaCred'
        ParamType = ptInput
        Value = '0=0'
      end
      item
        DataType = ftString
        Name = 'MEmpresa'
